{
	"name": "@maca134/ts-express-utils",
	"version": "1.0.11",
	"description": "Typescript utils for express",
	"main": "./dist",
	"scripts": {
		"generate-barrels": "barrelsby --delete -d ./src",
		"build": "rimraf ./dist && npm run generate-barrels && tsc",
		"git": "git add ./* & git commit -m \"auto-commit\" & git push",
		"update": "npm run build & npm run git & npm version patch & npm run git",
		"postversion": "npm publish"
	},
	"author": "maca134",
	"license": "UNLICENSED",
	"dependencies": {
		"express": "^4.17.1",
		"reflect-metadata": "^0.1.13"
	},
	"devDependencies": {
		"@types/express": "^4.17.2",
		"@types/node": "^12.12.12",
		"barrelsby": "^2.2.0",
		"rimraf": "^3.0.0",
		"ts-node": "^8.5.2",
		"typescript": "^3.7.2"
	}
}
